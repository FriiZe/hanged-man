{
  "version": 1,
  "projects": {
    "engine": {
      "root": "libs/engine",
      "sourceRoot": "libs/engine/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/engine/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/engine/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "cli": {
      "root": "apps/cli",
      "sourceRoot": "apps/cli/src",
      "projectType": "application",
      "prefix": "cli",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/cli",
            "main": "apps/cli/src/main.ts",
            "tsConfig": "apps/cli/tsconfig.app.json",
            "assets": ["apps/cli/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/cli/src/environments/environment.ts",
                  "with": "apps/cli/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "cli:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cli/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/cli/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "cli"
}
